{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "io.embrace.sdk/Scripts/**.cs"
          ],
          "src": "../",
          "exclude": [
            "io.embrace.sdk/Scripts/Embrace_Tests.cs",
            "io.embrace.sdk/Scripts/Embrace_Stub.cs",
            "io.embrace.sdk/Scripts/EmbraceSDKInfo.cs",
            "io.embrace.sdk/Scripts/IEmbraceProvider.cs",
            "io.embrace.sdk/Scripts/Native/**.cs",
            "io.embrace.sdk/Scripts/Utilities/ConfigParser.cs",
            "io.embrace.sdk/Scripts/Utilities/EmbraceLogger.cs",
            "io.embrace.sdk/Scripts/Utilities/TimeUtil.cs",
            "io.embrace.sdk/Scripts/Utilities/UnhandledException.cs",
            "io.embrace.sdk/Scripts/Utilities/UnhandledExceptionRateLimiting.cs",
            "io.embrace.sdk/Scripts/Data/RemoteConfig.cs"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "namespaceLayout": "flattened"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_disableContribution": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "./_exported_templates/default/"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}